{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Total Events",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "mysql", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "mysql" },
          "format": "table",
          "rawSql": "SELECT COUNT(*) AS total FROM security_event;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Events by Severity",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "mysql", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "mysql" },
          "format": "table",
          "rawSql": "SELECT severity, COUNT(*) AS count FROM security_event GROUP BY severity ORDER BY FIELD(severity, 'CRITICAL', 'HIGH', 'MEDIUM', 'LOW');",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "CRITICAL" },
            "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "HIGH" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "MEDIUM" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "LOW" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        },
        "pieType": "donut",
        "reduceOptions": {
          "values": true,
          "calcs": [],
          "fields": "/^count$/"
        },
        "tooltip": { "mode": "single" }
      },
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "count (.*)",
            "renamePattern": "$1"
          }
        }
      ]
    },
    {
      "id": 3,
      "title": "Events by Type",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "mysql", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "mysql" },
          "format": "table",
          "rawSql": "SELECT event_type, COUNT(*) AS count FROM security_event GROUP BY event_type ORDER BY count DESC;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisLabel": "",
            "fillOpacity": 80,
            "gradientMode": "none",
            "lineWidth": 1
          }
        },
        "overrides": []
      },
      "options": {
        "barWidth": 0.7,
        "fullHighlight": false,
        "groupWidth": 0.7,
        "legend": { "displayMode": "list", "placement": "bottom" },
        "orientation": "horizontal",
        "showValue": "always",
        "stacking": "none",
        "tooltip": { "mode": "single" },
        "xTickLabelRotation": 0
      }
    },
    {
      "id": 4,
      "title": "Alert Timeline",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "mysql", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "mysql" },
          "format": "time_series",
          "rawSql": "SELECT\n  UNIX_TIMESTAMP(DATE_FORMAT(created_at, '%Y-%m-%d %H:00:00')) AS time_sec,\n  COUNT(*) AS alerts\nFROM alert\nWHERE created_at >= NOW() - INTERVAL 24 HOUR\nGROUP BY time_sec\nORDER BY time_sec;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "title": "Recent Security Events",
      "type": "table",
      "gridPos": { "h": 10, "w": 18, "x": 0, "y": 8 },
      "datasource": { "type": "mysql", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "mysql" },
          "format": "table",
          "rawSql": "SELECT\n  id,\n  event_type,\n  severity,\n  source_ip,\n  detected_by,\n  confidence,\n  status,\n  description,\n  created_at\nFROM security_event\nORDER BY created_at DESC\nLIMIT 50;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "severity" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "CRITICAL": { "color": "dark-red", "text": "CRITICAL" } } },
                  { "type": "value", "options": { "HIGH": { "color": "red", "text": "HIGH" } } },
                  { "type": "value", "options": { "MEDIUM": { "color": "orange", "text": "MEDIUM" } } },
                  { "type": "value", "options": { "LOW": { "color": "green", "text": "LOW" } } }
                ]
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "description" },
            "properties": [
              { "id": "custom.width", "value": 300 }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "footer": { "show": false },
        "sortBy": []
      }
    },
    {
      "id": 6,
      "title": "Log Ingestion Rate",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 6, "x": 18, "y": 8 },
      "datasource": { "type": "elasticsearch", "uid": "" },
      "targets": [
        {
          "datasource": { "type": "elasticsearch" },
          "metrics": [
            {
              "id": "1",
              "type": "count"
            }
          ],
          "bucketAggs": [
            {
              "id": "2",
              "type": "date_histogram",
              "field": "@timestamp",
              "settings": {
                "interval": "10m",
                "min_doc_count": "0",
                "trimEdges": "0"
              }
            }
          ],
          "query": "*",
          "refId": "A",
          "timeField": "@timestamp"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "super-light-blue" },
          "custom": {
            "axisBorderShow": false,
            "drawStyle": "bars",
            "fillOpacity": 60,
            "gradientMode": "scheme",
            "lineWidth": 0,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["aisiem", "security"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "AI SIEM Dashboard",
  "uid": "aisiem-main",
  "version": 1
}
